#include<stdio.h>
int main (){
   int array[10] = {4,1,6,8,10,21,8,9,2,6};
   int min = array[0];
   int max = array[0];
   int i;
   
   for (i = 1; i < 10; i++){
       if (array[i] < min){
          min = array[i];
          }
        if (array[i] > max){
        max = array[i];
       } 
  }
        printf("Minimum value: %d\n", min);
        printf("Maximum value: %d\n", max);
    
        return 0;
        }
