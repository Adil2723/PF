#include <stdio.h>

int main() {
	char a ='A';
	for(int i=0;i<5;i++) {
		printf("%c ",a+i);
	}
	printf("\n");
	for(int i=2;i<6;i+=3) {
		printf(" %c   ",a+i);
	}
	printf("\n   %c\n",a+4);
	for(int i=4;i<8;i+=3) {
		printf(" %c   ",a+i);
	}
	printf("\n");
	for(int i=4;i<9;i++) {
		printf("%c ",a+i);
	}
Return 0;
}


