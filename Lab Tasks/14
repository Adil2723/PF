#include<stdio.h>
int main(){
int num1, num2 , num3;

 printf("Enter numbers: ");
 scanf("%d %d %d", &num1, &num2, &num3);

 if (num1 >= num3){
  if( num1 >= num2){
   printf("num1 is greatest\n");
   } else {
   printf("num2 is greatest\n");
}
 } else   {
     if (num2 > num3) {
     printf("num2 is the greatest\n");
     } else {
     printf("num3 is the greatest\n");
 }
} 
 return 0;
}
