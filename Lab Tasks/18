#include<stdio.h>
int main(){
int num, i, isPrime = 1;
       printf("Enter a number: ");
       scanf("%d", &num);

if (num < 2){
    isPrime = 0;
 }else{
    for(i = 2; i*i <= num; i++){
    if(num%i == 0){
    isPrime= 0;
    break;
    }
  }  
}    
if (isPrime= 1){
printf("Is a Prime number %d\n", num);
} else {
printf("Not a Prime number %d\n", num);
}
return 0;
}
